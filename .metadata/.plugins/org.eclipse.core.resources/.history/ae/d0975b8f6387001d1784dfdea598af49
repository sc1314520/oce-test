    pageEncoding="UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>forEach 標籤</title>
</head>
<body>
	<%
		request.setAttribute("number", 3);
	%>
	<c:forEach begin="1" end="10" var="i" step="1">
		${ i } <br>
	</c:forEach>
</body>
</html>